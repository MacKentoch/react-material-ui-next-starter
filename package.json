{
  "name": "react-material-next-starter",
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "version": "1.1.0",
  "description": "ReactJS + Material-UI Next starter",
  "main": "src/index.js",
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "predev": "node ./scripts/prepareIndexHtml.js",
    "dev": "rimraf docs/public/assets && cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "postdev": "react-snap",
    "preprod": "node ./scripts/prepareIndexHtml.js",
    "prod": "rimraf docs/public/assets && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "postprod": "react-snap",
    "analyze": "rimraf docs/public/assets && cross-env NODE_ENV=production webpack -p --config webpack.analyze.config.js",
    "preStart": "node ./scripts/prepareIndexHtml.js",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack.hot.reload.config.js --mode development",
    "serve-dev": "npm run dev && nodemon ./src/server/index.js",
    "serve-prod": "npm run prod && node ./src/server/index.js"
  },
  "reactSnap": {
    "source": "docs"
  },
  "engines": {
    "node": ">=8.12.0",
    "npm": "^6.4.1",
    "yarn": ">=1.10.1"
  },
  "keywords": [
    "material",
    "design",
    "material ui",
    "material ui next",
    "react",
    "ES6",
    "ES2015",
    "ES6+",
    "ES7",
    "starter",
    "webpack",
    "hot-reload",
    "react-hot-reload"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/classnames": "^2.2.6",
    "@types/compression": "^0.0.36",
    "@types/convict": "^4.2.0",
    "@types/date-fns": "^2.6.0",
    "@types/enzyme": "^3.1.14",
    "@types/express": "^4.16.0",
    "@types/express-promise-router": "^2.0.0",
    "@types/express-rate-limit": "^2.9.3",
    "@types/helmet": "^0.0.42",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/react-motion": "^0.0.27",
    "@types/reactstrap": "^6.4.2",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/rename": "^1.0.1",
    "@types/webpack": "^4.4.17",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-react": "^7.11.1",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^7.2.5",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.85.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jest-localstorage-mock": "^2.3.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.4",
    "modernizr-webpack-plugin": "^1.0.7",
    "nodemon": "^1.18.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.12",
    "react-snap": "^1.19.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.11.1",
    "rename": "^1.0.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.24.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "convict": "^4.4.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "express-rate-limit": "^3.3.0",
    "helmet": "^3.14.0",
    "history": "^4.7.2",
    "loadable-components": "^2.2.3",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "modernizr": "^3.6.0",
    "popper.js": "^1.14.4",
    "prettier-eslint": "^8.8.2",
    "pretty-error": "^2.1.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-motion": "^0.5.2",
    "react-overdrive": "^0.0.10",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-tap-event-plugin": "^3.0.3",
    "recompose": "^0.30.0",
    "serialize-javascript": "^1.5.0",
    "serve-favicon": "^2.5.0",
    "smoothscroll-polyfill": "^0.4.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setupTests.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__TESTS__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/__mocks__/fileMock.js",
      "^.+\\.(css|less|sass)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
